<launch>
    <node pkg="performance_monitor" type="performance_monitor.py" name="performance_monitor" output="screen">
        <param name="s_begin" type="double" value="0.01"/>
        <param name="s_end" type="double" value="10.0"/>
        <param name="file_name" type="string" value="performance_data.csv"/>

        <remap from="speed" to="model_speed"/>
    </node>
</launch>